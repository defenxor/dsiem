{
  "plugins": [ 
    "@semantic-release/commit-analyzer", 
    "@semantic-release/release-notes-generator", 
    [ "@semantic-release/github", {
        "assets": [
          {"path": "temp/release/latest/*" }
        ]
      }
    ],
    ["@semantic-release/exec", {
      "analyzeCommitsCmd": "touch ./releasetag",
      "verifyReleaseCmd": "echo ${nextRelease.version} > ./releasetag",
      "successCmd": "echo ${nextRelease.version} > ./releasetag"
    }]
  ]
}
